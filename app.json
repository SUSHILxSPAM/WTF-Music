{
  "name": "WTF Music",
  "description": "Bot to play songs in Telegram group voice chats. Powered by PyTgCalls.",
  "keywords": ["music", "voicechat", "telegram"],
  "logo": "https://telegra.ph/file/053f99956ccee8416b8f7.jpg",
  "repository": "https://github.com/AnonymousBoy1025/WTF-Music",
  "stack": "container",
  "env": {
    "SESSION_NAME": {
      "description": "Pyrogram session string of assistant account",
      "required": true
    },
    "BOT_TOKEN": {
      "description": "A bot token from @BotFather",
      "required": true
    },
    "API_ID": {
      "description": "API_ID from my.telegram.org/apps",
      "required": true
    },
    "API_HASH": {
      "description": "API_HASH from my.telegram.org/apps",
      "required": true
    },
    "OWNER_ID": {
      "description": "Your Telegram ID",
      "required": true
    }
  }
}
